import java.util.HashMap;

class Main{

    public static int romanToInt(String s){
	HashMap<String, Integer> RomanValues = new HashMap<String, Integer>();
	RomanValues.put("I", 1);
	RomanValues.put("V", 5);
	RomanValues.put("X", 10);
	RomanValues.put("L", 50);
	RomanValues.put("C", 100);
	RomanValues.put("D", 500);
	RomanValues.put("M", 1000);

	int total = 0;
	int pre_value = 0;

	String reversed = s.reversed();

	for(int i = 0; i<reversed.lenght(); i++){
	    int value = RomanValues.get(reversed[i]);

	    if(value < pre_value){
		total = total - value;
	    } else {
		total = total + value;
		prev_value = value;
	    }
	     
	}

	return total;
		
    }
    
    public static void main(String[] args){
	System.out.println("013 Roman to Integer : III, LVIII, MCMXCIV");
	String s1 = "III";
	String s2 = "LVIII";
	String s3 = "MCMXCIV"
	 
    }
}
