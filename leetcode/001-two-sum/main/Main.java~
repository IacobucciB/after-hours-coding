import java.util.Arrays;

class Main {

    public static int TwoSum (int a, int b) {
	return (a + b);
    }
    
    public static void main (String[] args){
	System.out.println("Hello World");
	int a = 5;
	int b = 2;
	int c = TwoSum(a , b);
	System.out.println(c);

	int[] eg1 = {2,7,11,15};
	int tgt1 = 9;

	int[] eg2 = {3,2,4};
	int tgt2 = 6;

	int[] eg3 = {3,3};
	int tgt3 = 6;

	System.out.println(Arrays.toString(eg1));
    }
}
